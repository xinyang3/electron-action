<!DOCTYPE html>

<head>
  <meta charset="utf-8">
  <title><%= htmlWebpackPlugin.options.title %></title>
  <!-- Add `node_modules/` to global paths so `require` works properly in development -->
  <link rel="stylesheet" href="<%= htmlWebpackPlugin.options.base %>static/css/base.css">
  </link>
  <link rel="stylesheet" href="<%= htmlWebpackPlugin.options.base %>static/css/common.css">
  </link>
  <link rel="stylesheet" href="<%= htmlWebpackPlugin.options.base %>static/css/iconfont/iconfont.css">
  </link>
  <% if (htmlWebpackPlugin.options.nodeModules) { %>
  <!-- Add `node_modules/` to global paths so `require` works properly in development -->
  <script>
    require('module').globalPaths.push("<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>")
  </script>
  <% } %>
</head>

<body>
  <div id="app"></div>
  <div class="operate-menu no-drag">
    <span><i id="min" class="min icon iconfont icon-min"></i></span>
    <span><i id="max" class="max icon iconfont icon-max"></i></span>
    <span><i id="hidden" class="close icon iconfont icon-close"></i></span>
  </div>
  <!--webpack builds are automatically injected-->
</body>
<script src="<%= htmlWebpackPlugin.options.base %>static/css/iconfont/iconfont.js"></script>
<% if (!process.browser) { %>
<script>
  if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '../static').replace(
    /\\/g, '\\\\')
</script>
<% } %>

</html>